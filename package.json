  "type": "module",
  "dependencies": {
    "@studio-freight/lenis": "^1.0.27",
    "@tanstack/svelte-query": "^5.29.0",
    "@wagmi/connectors": "^4.1.25",
    "@wagmi/core": "^2.6.16",
    "@web3modal/wagmi": "^4.1.5",
    "gsap": "^3.12.2",
    "motion": "^10.16.4",
    "split-type": "^0.3.4",
    "three": "^0.157.0",
    "viem": "^2.9.9",
    "wagmi": "^2.5.19"
  }
}
